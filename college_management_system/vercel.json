{

    "builds": [{

        "src": "college_managemnet_system/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "college_managemnet_system/wsgi.py"
        }
    ]
